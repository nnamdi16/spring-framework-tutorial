<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<!--    Definition of JavaCollection-->
    <bean id="javaCollection" class="com.JavaCollection.JavaCollection">
<!--        Result in a setAddressList(java.util.List) call-->
        <property name="addressList">
            <list>
                <value>india</value>
                <value>Pakistan</value>
                <value>USA</value>
                <value>USA</value>
            </list>
        </property>
        <property name="addressSet">
            <set>
                <value>INDIA</value>
                <value>Pakistan</value>
                <value>USA</value>
                <value>USA</value>
            </set>
        </property>
        <!--        Result in a setAddressSet(java.util.Set) call-->
        <property name="addressMap">
            <map>
                <entry key="1" value="INDIA"/>
                <entry key="2" value="Pakistan"/>
                <entry key="3" value="USA"/>
                <entry key="4" value="USA"/>
            </map>
        </property>

        <!--        Result in a setAddressProp(java.util.Prop) call-->
        <property name="addressProp">
            <props>
                <prop key="one">INDIA</prop>
                <prop key="Two">Pakistan</prop>
                <prop key="Three">USA</prop>
                <prop key="Four">USA</prop>
            </props>
        </property>
    </bean>
</beans>